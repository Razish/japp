				<Option type="3" />
				<Option compiler="clang" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-DNDEBUG" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="JA" value="~/ja" />
			</Environment>
		</Build>
		<Compiler>
