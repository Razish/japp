<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ui" />
		<Option pch_mode="2" />
		<Option compiler="clang" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/uix86_64" prefix_auto="0" extension_auto="1" />
