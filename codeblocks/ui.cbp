			<Add option="-Wold-style-definition" />
			<Add option="-Wstrict-prototypes" />
			<Add option="-Wno-missing-braces" />
			<Add option="-Wno-missing-field-initializers" />
			<Add option="-Wno-sign-compare" />
			<Add option="-Wno-unused-parameter" />
			<Add option="-Waggregate-return" />
			<Add option="-Winit-self" />
			<Add option="-Winline" />
			<Add option="-Wmissing-include-dirs" />
			<Add option="-Woverlength-strings" />
			<Add option="-Wpointer-arith" />
			<Add option="-Wredundant-decls" />
			<Add option="-Wundef" />
			<Add option="-Wuninitialized" />
			<Add option="-Wwrite-strings" />
			<Add option="-fvisibility=hidden" />
			<Add option="-msse2" />
			<Add option="-mstackrealign" />
			<Add option="-fPIC" />
			<Add option="-D_UI" />
			<Add option="-DJPLUA" />
			<Add directory="../" />
			<Add directory="../game" />
			<Add directory="../ui" />
		</Compiler>
		<ExtraCommands>
			<Add after="cp $(TARGET_OUTPUT_FILE) $(JA)/japlus" />
